{
  "name": "avian",
  "version": "0.0.24",
  "description": "Create Enterprise-class component driven applications that scale.",
  "repository": {
    "type": "git",
    "url": "https://github.com/flypapertech/avian.git"
  },
  "preferGlobal": true,
  "main": "./dist/avian.cli.js",
  "bin": {
    "avian": "./dist/avian.cli.js"
  },
  "scripts": {
    "prepare": "npm run build_cli && npm run build_spec && npm run test",
    "build_cli": "node_modules/.bin/tsc ./src/avian.cli.ts &>/dev/null ; node_modules/.bin/uglifyjs ./src/avian.cli.js -b -o ./src/avian.cli.js ; node_modules/.bin/uglifyjs ./src/avian.cli.js -o ./dist/avian.cli.js",
    "build_spec": "node_modules/.bin/tsc ./src/avian.spec.ts &>/dev/null ; node_modules/.bin/uglifyjs ./src/avian.spec.js -b -o ./src/avian.spec.js ; node_modules/.bin/uglifyjs ./src/avian.spec.js -o ./spec/avian.spec.js",
    "test": "node node_modules/.bin/jasmine -w",
    "start": "node dist/avian.cli.js"
  },
  "contributors": [
    "Dan Stephenson",
    "Nick Fredricks"
  ],
  "license": "MIT",
